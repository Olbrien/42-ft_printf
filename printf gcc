gcc ft_printf.c main.c sources/*.c sources/specifiers/*.c sources/specifiers/flags/*.c sources/conversions/*.c sources/conversions/lengths/*.c libft/*.c


gcc -g -Og -std=gnu99 ft_printf.c main.c sources/*.c sources/specifiers/*.c sources/specifiers/flags/*.c sources/conversions/*.c sources/conversions/lengths/*.c libft/*.c  -o memoryleak
valgrind --leak-check=full ./memoryleak
valgrind --leak-check=full --track-origins=yes --show-leak-kinds=all -v ./memoryleak


